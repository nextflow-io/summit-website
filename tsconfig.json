{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@modules/*": ["src/modules/*"],
      "@data/*": ["src/data/*"],
      "@utils/*": ["src/utils/*"],
      "@images/*": ["src/images/*"]
    }
  }
}