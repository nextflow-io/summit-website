---
import Layout from '@layouts/Base.astro';
import SEO from '@components/SEO';
import StandardPage from "@modules/StandardPage"
import { fetchPastEvents } from '@data/fetchers';
import type { PageData } from '@data/types';

const past: PageData = await fetchPastEvents();

---

<Layout>
  <SEO slot="head" title="Past Events" />
  <StandardPage data={past}  client:load/>
</Layout>
