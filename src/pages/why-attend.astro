---
import Layout from '@layouts/Base.astro';
import SEO from '@components/SEO';
import StandardPage from "@modules/StandardPage"
import { fetchWhyAttend } from '@data/fetchers';
import type { PageData } from '@data/types';

const attend: PageData = await fetchWhyAttend();

---

<Layout>
  <SEO slot="head" title="Why Attend" />
  <StandardPage data={attend} client:load/>
</Layout>
