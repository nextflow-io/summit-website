---
import Layout from '@layouts/Base.astro';
import SEO from '@components/SEO';
import Hackathon from "@modules/Hackathon"


import { fetchBostonHackathon} from '@data/fetchers';
import type { PageData } from '@data/types';

const hackathon: PageData = await fetchBostonHackathon();

---

<Layout>
  <SEO slot="head" title="Hackathon | Boston" />
  <Hackathon hackathon={hackathon}  client:load/>
</Layout>
