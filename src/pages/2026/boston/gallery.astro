---
import Layout from "@layouts/Base.astro";
import SEO from "@components/SEO";
import OverviewPage from "@modules/Overview"
import { fetchBostonGallery} from '@data/fetchers';
import type { PageData } from '@data/types';


const gallery: PageData = await fetchBostonGallery();

---

<Layout namespace="2026/boston" altBG>
  <SEO slot="head" title="Gallery" img={1} />
    <OverviewPage overview={gallery}  client:load/>
</Layout>

