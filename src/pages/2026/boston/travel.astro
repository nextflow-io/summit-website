---
import Layout from '@layouts/Base.astro';
import SEO from '@components/SEO';
import TravelPage from "@modules/Travel"
import { fetchBostonTravel} from '@data/fetchers';
import type { PageData } from '@data/types';

const travel: PageData = await fetchBostonTravel();


---

<Layout>
  <SEO slot="head" title="Travel | Boston" />
  <TravelPage travel={travel} client:load/>
</Layout>
