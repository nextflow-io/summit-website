---
import Layout from '@layouts/Base.astro';
import SEO from '@components/SEO';
import Overview from "@modules/Overview"
import {fetchBostonOverview} from "@data/overview";

const overview = await fetchBostonOverview();
---

<Layout showHeader={false}>
  <SEO slot="head" title="Home" />
  <Overview overview={overview}  client:load/>
</Layout>
