---
import Layout from "@layouts/Base.astro";
import SEO from "@components/SEO";
import StandardPage from "@modules/StandardPage"
import { fetchCFA} from '@data/fetchers';
import type { PageData } from '@data/types';

const cfa: PageData = await fetchCFA();
---

<Layout>
  <SEO slot="head" title="Call for Speakers and Posters" img={3} />
  <StandardPage data={cfa}  client:load/>
</Layout>
