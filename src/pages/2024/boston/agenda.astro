---
import Layout from "@layouts/Base.astro";
import CallForAbstracts from "@modules/CallForAbstracts";
import KeyInformation from "@modules/KeyInformation";
import ImageSection from "@modules/ImageSection";
import CTASection from "@modules/CTASection";
import Contact from "@modules/Contact";
import SEO from "@components/SEO";
---

<Layout namespace="2024/boston">
  <SEO slot="head" title="Agenda" img={1} />
  <section class="pb-6 pt-16 md:pt-20">
    <CallForAbstracts client:load className="pb-10 md:pb-16" />
    <div class="container island pb-10 md:pb-20 text-center">
      <h1 class="h00 mb-8">Join us in Boston</h1>
      <div class="blockquote">
        This premier event brings together leading experts, innovators, and
        researchers to showcase the latest breakthroughs in workflow management.
      </div>
    </div>
    <KeyInformation />
    <ImageSection alt1 className="mt-20" />
  </section>
  <CTASection />
  <Contact />
</Layout>
